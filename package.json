{
  "name": "hapi-greenkeeper-keeper",
  "version": "0.0.0-semantically-released",
  "main": "lib/plugin.cjs.js",
  "module": "lib/plugin.es.js",
  "license": "MIT",
  "author": "Kurtis Kemple <kurtiskemple@gmail.com>",
  "maintainers": [
    "Kurtis Kemple <kurtiskemple@gmail.com>",
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "description": "hapi plugin to automatically merge and clean up passing greenkeeper PRs",
  "repository": "greenkeeper-keeper/hapi-greenkeeper-keeper",
  "bugs": "https://github.com/greenkeeper-keeper/hapi-greenkeeper-keeper/issues",
  "homepage": "https://github.com/greenkeeper-keeper/hapi-greenkeeper-keeper#readme",
  "keywords": [
    "greenkeeper.io",
    "github",
    "hapi",
    "plugin"
  ],
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit:no-coverage": "mocha --recursive test/unit",
    "coverage": "nyc run-s tests:unit:no-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "tests:unit": "run-s coverage",
    "tests:integration": "cucumber.js test/integration --compiler js:babel-register --tags 'not @wip'",
    "tests:integration:debug": "DEBUG=test run-s tests:integration",
    "tests:integration:wip": "cucumber.js test/integration --compiler js:babel-register --tags @wip",
    "test": "run-s lint:* tests:*",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepublish": "run-s build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@travi/any": "1.2.1",
    "@travi/eslint-config-travi": "1.2.17",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-stage-3": "6.24.1",
    "chai": "4.0.2",
    "chai-as-promised": "7.1.1",
    "codecov": "2.2.0",
    "cucumber": "2.3.1",
    "cz-conventional-changelog": "2.0.0",
    "debug": "2.6.8",
    "globstar": "1.0.0",
    "good": "7.2.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.2",
    "hapi": "16.4.3",
    "husky": "0.14.2",
    "markdownlint-cli": "0.3.0",
    "mocha": "3.4.1",
    "nock": "9.0.13",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.3",
    "proxyquire": "1.8.0",
    "rollup": "0.42.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "semantic-release": "6.3.6",
    "sinon": "2.3.6",
    "validate-commit-msg": "2.12.2"
  },
  "dependencies": {
    "boom": "5.1.0",
    "btoa": "1.1.2",
    "delay": "2.0.0",
    "highwire": "1.1.6",
    "hoek": "4.1.1",
    "http-status-codes": "1.1.6",
    "joi": "10.6.0",
    "lodash.groupby": "4.6.0",
    "lodash.values": "4.3.0",
    "milliseconds": "1.0.3"
  }
}
